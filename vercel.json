{
  "version": 2,
  "name": "cursor-skills",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
          "routes": [
            {
              "src": "/php/(.*)",
              "dest": "/environments/php/index.html"
            },
            {
              "src": "/webdesign/(.*)",
              "dest": "/environments/webdesign/index.html"
            },
            {
              "src": "/python/(.*)",
              "dest": "/environments/python/index.html"
            },
            {
              "src": "/node/(.*)",
              "dest": "/environments/node/index.html"
            },
            {
              "src": "/api/(.*)",
              "dest": "/environments/api/index.html"
            },
            {
              "src": "/integrations/(.*)",
              "dest": "/environments/integrations/index.html"
            },
            {
              "src": "/mobile/(.*)",
              "dest": "/environments/mobile/index.html"
            },
            {
              "src": "/devops/(.*)",
              "dest": "/environments/devops/index.html"
            },
            {
              "src": "/testing/(.*)",
              "dest": "/environments/testing/index.html"
            },
            {
              "src": "/templates/(.*)",
              "dest": "/templates/$1"
            },
            {
              "src": "/examples/(.*)",
              "dest": "/examples/$1"
            },
            {
              "src": "/configs/(.*)",
              "dest": "/configs/$1"
            },
            {
              "src": "/docs/(.*)",
              "dest": "/docs/$1"
            },
            {
              "src": "/(.*)",
              "dest": "/index.html"
            }
          ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null
}
